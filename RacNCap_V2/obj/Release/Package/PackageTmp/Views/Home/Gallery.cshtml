            /*background-color: #485f77*/
        }

        .content {
            color: #777;
            /*font: 12px/1.4 "helvetica neue",arial,sans-serif;*/
            width: 1400px;
            margin-left: -11%;
            position: relative;
        }

        h1 {
            font-size: 12px;
            font-weight: normal;
            color: #ddd;
            margin: 0;
        }

        p {
            margin: 0 0 20px;
        }

        a {
            color: #22BCB9;
            text-decoration: none;
        }

        .cred {
            margin-top: 20px;
            font-size: 11px;
        }

        /* This rule is read by Galleria to define the gallery height: */
        #galleria {
            height: 720px;
        }
    </style>

    <!-- load jQuery -->
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.js"></script>

    <!-- load Galleria -->
